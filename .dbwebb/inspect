#
# Inspect, tools to inspect a kmom.
#
# Usage: dbwebb inspect kmom01
#
# Sequence executed:
# webapp(), webappkmom01(), webapplast()
#


#
# Test webapp general
#
webapp()
{
    inspectIntro
}



#
# Test general, last in sequence
#
webapplast()
{
    inspectMe "me/redovisa"
}



#
# Test webapp kmom01
#
webappkmom01()
{
    inspectExerciseHeader "lager1" "uppgift/lager-appen-del-1" "$KMOM/lager1"
    printUrl "" "me/$KMOM/lager1"
}



#
# Test webapp kmom02
#
webappkmom02()
{
    inspectLab "uppgift/sokverktyg-for-json-filer" "jq" "answer.bash" "bash answer.bash"


    inspectExerciseHeader "lager2" "uppgift/lager-appen-del-2" "$KMOM/lager2"
    printUrl "" "me/$KMOM/lager2"
}



